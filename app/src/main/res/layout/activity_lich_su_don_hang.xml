<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.LichSuDonHangActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarManHinhLichSuDonHang"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="#ECBB29"
        app:layout_constraintTop_toTopOf="parent"
        app:title="Lịch Sử Đơn hàng"
        app:titleTextColor="@color/white"
        tools:layout_editor_absoluteX="0dp">

    </androidx.appcompat.widget.Toolbar>
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rcvLichSuDonHang"
        app:layout_constraintTop_toBottomOf="@+id/toolbarManHinhLichSuDonHang"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="match_parent"
        android:layout_height="0dp"></androidx.recyclerview.widget.RecyclerView>
</androidx.constraintlayout.widget.ConstraintLayout>